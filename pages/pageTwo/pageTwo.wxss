page{
  height:100%;
}
.containerTwo-box{
  width:100%;
  height:100%;
  background-size:  cover; 
  background-repeat:  no-repeat;
   position:relative;    
}
/*音乐图标*/
.container-playMusic{
  width:60rpx;
  height:60rpx;
  position:absolute;
  z-index:99;
  right:36rpx;
  top:30rpx;
  animation:rotateMusic 2s linear infinite;
}
@keyframes rotateMusic{
  0%{
    transform:rotate(0deg);
  }
  50%{
    transform:rotate(180deg);
  }
  100%{
    transform:rotate(360deg);
  }
}
/*顶部图片*/
.containerTwo-oneTopImg{
  width:100%;
  height:700rpx;

}

