}
/* 底部文字图片部分 */
.containerTwo-oneBottomTextImgO{
  width:360rpx;
  position:absolute;
  top:-100rpx; 
  left:180rpx;
  z-index:100;
  animation:textLate 1.5s linear forwards;
}
@keyframes textLate{
  0%{
    transform:translateY(0rpx);
  }
  70%{
    transform:translateY(940rpx);
  }
  85%{
    transform:translateY(870rpx);
  } 
  100%{
    transform:translateY(900rpx);
  }
}

/* 底部第二张图 */
.containerTwo-oneBottomTextImgT{
  width:360rpx;
  position:absolute;
  top:1200rpx;
  left:180rpx;
  z-index:100;
  animation:textImgTLate 1.5s linear forwards;
}
 @keyframes textImgTLate{
  0%{
    transform:translateY(0rpx);
  }

  70%{
    transform:translateY(-260rpx);
  }
  85%{
    transform:translateY(-210rpx);
  }
  100%{
    transform:translateY(-240rpx);
  }
} 

/* 箭头图标 */
.container-arrow{
  width:100rpx;
  height:80rpx;
  position:absolute; 
  top:1100rpx;
  left:320rpx;
  opacity:1;
  z-index:999;
  animation:arrowImg 2s linear infinite;
}
@keyframes arrowImg{
  0%{
    opacity:0;
  }
  5%{
    opacity:0.5;
    transform:translateY(-10rpx);
  }
  50%{
    opacity:1;
    transform:translateY(-60rpx);
  }
  100%{
    transform:translateY(-80rpx);
  }
}

