/* 更多页 */
.threeLastBlankBox{
  width:100%;
  height:910rpx;
}

.threeLastBtnBox{
  width:100%;  
  text-align:center;
}
 .threeMoreBtnImg{
  width:320rpx;
  height:100rpx;
  margin:0 auto;
  line-height:100rpx;
  animation:twoBtnShaking 2s linear forwards;
} 
@keyframes twoBtnShaking{
 20%{
  transform:scale(1.2);
 }
 30%{
   transform:rotate(-6deg) scale(1.2);
 }
 40%{
   transform:rotate(6deg) scale(1.2);
 }
  50%{
   transform:rotate(-6deg) scale(1.2);
 }
 60%{
   transform:rotate(6deg) scale(1.2);
 }
 70%{
   transform:rotate(-6deg) scale(1.2);
 }
 80%{
   transform:rotate(6deg) scale(1.2);
 }
 100%{
   transform:scale(0);
   transform:rotate(0deg);
  }
}




